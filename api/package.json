    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cron": "^4.3.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.0",
    "grammy": "^1.21.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "joi": "^18.0.1",
    "lodash": "^4.17.21",
    "mammoth": "^1.7.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.1.1",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.10",
    "openai": "^6.8.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
